{
  "name": "online-gallery",
  "version": "0.0.1",
  "homepage": "./",
  "license": "MIT",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "prettier": "prettier --write **/*.{js,jsx,json}",
    "format": "yarn prettier && yarn lint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "yarn format",
      "git add ."
    ]
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1",
    "axios": "^0.21.1",
    "connected-react-router": "^6.9.1",
    "history": "^5.0.0",
    "lodash": "^4.17.21",
    "pixi-filters": "^4.1.2",
    "pixi.js": "^6.0.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-device-detect": "^1.17.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.2.0",
    "scrollmagic": "^2.0.8",
    "seamless-immutable": "^7.1.4",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@craco/craco": "^6.1.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0"
  }
}
